{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Dovotori",
      "url": "https://localhost:8080",
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}",
      "runtimeArgs": [
        "--ignore-certificate-errors",
        "--unsafely-treat-insecure-origin-as-secure=https://localhost:8080"
      ]
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Labo",
      "url": "https://localhost:8081",
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}",
      "runtimeArgs": [
        "--ignore-certificate-errors",
        "--unsafely-treat-insecure-origin-as-secure=https://localhost:8081"
      ]
    }
  ]
}