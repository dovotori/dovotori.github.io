		"jest": "^29.7.0",
		"ol": "^10.4.0",
		"prettier": "^3.4.2",
		"puppeteer": "^24.1.1",
		"semver": "^7.7.0",
		"simple-peer": "^9.11.1",
		"socket.io-client": "^4.8.1",
