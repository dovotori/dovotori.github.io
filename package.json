{
  "name": "dovotori",
  "version": "1.1.1",
  "description": "React version of my portfolio website",
  "author": {
    "name": "Dorian Ratovo",
    "email": "dovotori@proton.me",
    "url": "https://dovotori.gitlab.io"
  },
  "license": "CC-BY-4.0",
  "scripts": {
    "start": "webpack serve --config webpack/dev.js",
    "build": "webpack --config webpack/prod.js",
    "clean": "node scripts/cleanBuild.js",
    "labo:start": "cross-env NAME=webgpu webpack serve --config webpack/labo.dev.js",
    "labo:build": "webpack --config webpack/labo.prod.js",
    "local:server": "npx http-server build/ -p 8080 -b -g --cors",
    "prettier": "prettier --write \"{src,labo,webpack,scripts,public}/**/*.{js,jsx}\"",
    "eslint": "eslint --fix --config .eslintrc --ext .jsx,.js src/ labo/ webpack/ scripts/ public/shaders/",
    "lint": "npm run prettier && npm run eslint",
    "favicon": "node scripts/favicons.js",
    "test": "jest --config=jest.config.js",
    "upgrade": "npx npm-check -u",
    "changelog": "npx auto-changelog -p",
    "docker:build:dev": "docker build --no-cache --target client_dev --tag dovotori_client_dev:latest .",
    "docker:run:dev": "docker run --name dovotori_client_dev -p 8080:8080 -d --rm dovotori_client_dev:latest",
    "docker:build:prod": "docker build --no-cache --target client_prod --tag dovotori_client_prod:latest .",
    "docker:run:prod": "docker run --name dovotori_client_prod --rm dovotori_client_prod:latest",
    "deploy": "gh-pages -d build --remote github",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=22.1.0"
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@reduxjs/toolkit": "^2.2.3",
    "@svgr/webpack": "^8.1.0",
    "babel-loader": "^9.1.3",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.1",
    "csv-parser": "^3.0.0",
    "earcut": "^2.2.4",
    "favicons": "^7.2.0",
    "file-loader": "^6.2.0",
    "geojson-vt": "^3.2.1",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "loader-css": "^0.1.0",
    "ol": "^9.1.0",
    "puppeteer": "^22.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.10.2",
    "react-redux": "^9.1.2",
    "react-router": "^6.23.0",
    "react-router-dom": "^6.23.0",
    "redux-thunk": "^3.1.0",
    "simple-peer": "^9.11.1",
    "socket.io-client": "^4.7.5",
    "style-loader": "^4.0.0",
    "styled-components": "^6.1.9",
    "sw-cache-plugin": "^1.1.0",
    "terser-webpack-plugin": "^5.3.10",
    "url-loader": "^4.1.1",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webrtc-adapter": "^9.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.5",
    "@storybook/addon-actions": "^8.0.10",
    "@storybook/addon-essentials": "^8.0.10",
    "@storybook/addon-interactions": "^8.0.10",
    "@storybook/addon-links": "^8.0.10",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/builder-webpack5": "^8.0.10",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^8.0.10",
    "@storybook/testing-library": "^0.2.2",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "gh-pages": "^6.1.1",
