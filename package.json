{
  "name": "dovotori-site",
  "version": "1.1.1",
  "description": "React version of my portfolio website",
  "author": {
    "name": "Dorian Ratovo",
    "email": "dratovo@gmail.com",
    "url": "https://dovotori.gitlab.io"
  },
  "license": "CC-BY-4.0",
  "scripts": {
    "start": "webpack-dev-server --config webpack/dev.js",
    "build": "webpack --config webpack/prod.js",
    "clean": "node scripts/cleanBuild.js",
    "labo:start": "webpack-dev-server --config webpack/labo.dev.js",
    "labo:build": "webpack --config webpack/labo.prod.js",
    "labo:all": "webpack --config webpack/labo.all.js",
    "local:server": "npx http-server build/",
    "lint": "npx prettier --write \"{src,labo,webpack,scripts}/**/*.{js,jsx}\" && eslint --fix --config .eslintrc --ext .jsx,.js src/ labo/ webpack/ scripts/",
    "favicon": "node scripts/favicons.js",
    "test": "jest --config=jest.config.js",
    "docker:build:dev": "docker build --no-cache --target client_dev --tag dovotori_client_dev:latest .",
    "docker:run:dev": "docker run --name dovotori_client_dev -p 8080:8080 -d --rm dovotori_client_dev:latest",
    "docker:build:prod": "docker build --no-cache --target client_prod --tag dovotori_client_prod:latest .",
    "docker:run:prod": "docker run --name dovotori_client_prod --rm dovotori_client_prod:latest"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@svgr/webpack": "5.1.0",
    "babel-loader": "8.0.6",
    "compression-webpack-plugin": "3.1.0",
    "copy-webpack-plugin": "5.1.1",
    "file-loader": "5.0.2",
    "geojson-vt": "^3.2.1",
    "history": "4.10.1",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "3.2.0",
    "intersection-observer": "0.7.0",
    "loader-css": "^0.1.0",
    "ol": "6.3.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-intersection-observer": "8.25.2",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-transition-group": "4.3.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "serviceworker-webpack-plugin": "1.0.1",
    "style-loader": "1.2.1",
    "styled-components": "5.0.1",
    "terser-webpack-plugin": "2.3.5",
    "url-loader": "3.0.0",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "babel-jest": "26.0.1",
    "css-loader": "3.5.3",
    "csv-parser": "2.3.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "favicons": "6.0.0",
    "jest": "25.1.0",
    "prettier": "2.0.5",
    "puppeteer": "3.3.0",
    "simple-peer": "^9.7.2",
    "socket.io-client": "^2.3.0",
    "standard-version": "7.1.0",
    "webpack-bundle-analyzer": "3.6.1",
    "webpack-dev-server": "3.10.3",
    "webrtc-adapter": "7.6.3"
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/dovotori/dovotori.gitlab.io.git"
  },
  "keywords": [
    "dovotori",
    "website",
    "portfolio",
    "dorian"
  ],
  "bugs": {
    "url": "https://gitlab.com/dovotori/dovotori.gitlab.io/issues",
