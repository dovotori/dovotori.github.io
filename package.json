    "earcut": "^2.2.2",
    "favicons": "6.2.0",
    "file-loader": "6.1.0",
    "geojson-vt": "^3.2.1",
    "gh-pages": "^3.1.0",
    "history": "4.10.1",
    "html-loader": "^1.3.1",
    "html-webpack-plugin": "4.5.0",
    "intersection-observer": "0.11.0",
    "loader-css": "^0.1.0",
    "ol": "6.4.3",
    "puppeteer": "5.3.1",
    "react": "16.13.1",
