    "style-loader": "1.2.1",
    "styled-components": "5.2.0",
    "terser-webpack-plugin": "4.2.2",
    "url-loader": "4.1.0",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "3.3.12",
    "webrtc-adapter": "7.7.0"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
