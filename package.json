    "favicons": "^4.8.2",
    "file-loader": "^6.2.0",
    "geojson-vt": "^3.2.1",
    "gh-pages": "^3.1.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.0.0-alpha.16",
