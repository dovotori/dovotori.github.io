{
  "name": "dovotori-site",
  "version": "1.0.0",
  "description": "React version of my portfolio website",
  "author": "Dorian R. <dratovo@gmail.com> (https://dovotori.fr.nf)",
  "license": "CC-BY-4.0",
  "scripts": {
    "start": "webpack-dev-server -r dotenv/config --config webpack/dev.js",
    "build": "webpack -r dotenv/config --config webpack/prod.js",
    "lint": "npx eslint --fix --ext .jsx,.js src/ webpack/ scripts/",
    "favicon": "node scripts/favicons.js",
    "test": "npm run lint && jest",
    "docker:build:dev": "docker build --no-cache --target client_dev --tag dovotori_client_dev:latest .",
    "docker:run:dev": "docker run --name dovotori_client_dev -p 8080:8080 -v /c/Users/Dovotori/git/portfolio/client/src:/client/src -d --rm dovotori_client_dev:latest",
    "docker:build:prod": "docker build --no-cache --target client_prod --tag dovotori_client_prod:latest .",
    "docker:run:prod": "docker run --name dovotori_client_prod -d --rm dovotori_client_prod:latest"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "dependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@svgr/webpack": "5.1.0",
    "babel-loader": "8.0.6",
    "compression-webpack-plugin": "3.1.0",
    "copy-webpack-plugin": "5.1.1",
    "file-loader": "5.0.2",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "3.2.0",
    "intersection-observer": "0.7.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-intersection-observer": "8.25.2",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-spring": "8.0.27",
    "react-transition-group": "4.3.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "serviceworker-webpack-plugin": "1.0.1",
    "styled-components": "5.0.1",
    "terser-webpack-plugin": "2.3.5",
    "url-loader": "3.0.0",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "favicons": "6.0.0",
    "jest": "25.1.0",
    "standard-version": "7.1.0",
    "webpack-dev-server": "3.10.3"
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/dovotori/website.git"
  },
  "keywords": [
    "dovotori",
    "website"
  ],
  "bugs": {
    "url": "https://gitlab.com/dovotori/website/issues",
    "email": "dratovo@gmail.com"
  },
  "homepage": "https://gitlab.com/dovotori/website#readme"
}