{
  "name": "dovotori",
  "version": "1.1.6",
  "description": "React version of my portfolio website",
  "author": {
    "name": "Dorian Ratovo",
    "email": "dovotori@proton.me",
    "url": "https://dovotori.gitlab.io"
  },
  "license": "CC-BY-4.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run start\" \"npm run storybook:dev\"",
    "start": "webpack serve --config webpack/dev.js",
    "images": "node scripts/convert-images.js",
    "build": "webpack --config webpack/prod.js",
    "clean": "node scripts/cleanBuild.js",
    "labo:start": "cross-env NAME=skin webpack serve --config webpack/labo.dev.js",
    "labo:build": "webpack --config webpack/labo.prod.js",
    "local:serve": "npx http-server build/ -p 8080 -b -g --cors",
    "lint": "biome check --write --enforce-assist=true --no-errors-on-unmatched && biome check --files-ignore-unknown=true",
    "favicon": "node scripts/favicons.js",
    "test": "jest --config=jest.config.js",
    "upgrade": "npx npm-check-updates -u",
    "changelog": "npx auto-changelog -p",
    "docker:build:dev": "docker build --no-cache --target client_dev --tag dovotori_client_dev:latest .",
    "docker:run:dev": "docker run --name dovotori_client_dev -p 8080:8080 -d --rm dovotori_client_dev:latest",
    "docker:build:prod": "docker build --no-cache --target client_prod --tag dovotori_client_prod:latest .",
    "docker:run:prod": "docker run --name dovotori_client_prod --rm dovotori_client_prod:latest",
    "deploy": "gh-pages -d build --remote origin",
    "release": "node scripts/release.js",
    "storybook:dev": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build -o build/storybook",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=22.13.1"
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@svgr/webpack": "^8.1.0",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.5",
    "loader-css": "^0.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-intersection-observer": "^10.0.0",
    "react-router": "^7.11.0",
    "react-router-dom": "^7.11.0",
    "style-loader": "^4.0.0",
    "styled-components": "^6.1.19",
    "sw-cache-plugin": "^1.1.0",
    "terser-webpack-plugin": "^5.3.16",
    "url-loader": "^4.1.1",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "@biomejs/biome": "2.3.10",
    "@chromatic-com/storybook": "^4.1.3",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.2",
    "@storybook/react": "^10.1.11",
    "@storybook/react-webpack5": "^10.1.11",
    "@storybook/test": "^8.6.14",
    "@storybook/testing-library": "^0.2.2",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@webgpu/types": "^0.1.68",
    "babel-jest": "^30.2.0",
    "compression-webpack-plugin": "^11.1.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "csv-parser": "^3.2.0",
    "earcut": "^3.0.2",
    "favicons": "^7.2.0",
    "geojson-vt": "^4.0.2",
    "gh-cli": "^0.0.1",
    "gh-pages": "^6.3.0",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "ol": "^10.7.0",
    "prettier": "^3.7.4",
    "puppeteer": "^24.34.0",
    "semver": "^7.7.3",
    "sharp": "^0.34.5",
    "simple-peer": "^9.11.1",
    "socket.io-client": "^4.8.3",
    "storybook": "^10.1.11",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack-bundle-analyzer": "^5.1.0",
    "webpack-dev-server": "^5.2.2",
    "webrtc-adapter": "^9.0.3"
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/dovotori/dovotori.gitlab.io.git"
  },
  "keywords": [
    "dovotori",
    "website",
    "portfolio",
    "dorian"
  ],
  "bugs": {
    "url": "https://gitlab.com/dovotori/dovotori.gitlab.io/issues",
    "email": "dovotori@proton.me"
  },
  "homepage": "https://dovotori.gitlab.io",
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --write --enforce-assist=true --no-errors-on-unmatched",
      "biome check --files-ignore-unknown=true"
    ],
    "*": [
      "biome check --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  }
}
